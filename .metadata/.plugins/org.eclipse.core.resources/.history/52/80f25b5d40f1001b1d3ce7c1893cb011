// 054. 향상된 for(foreach)로 배열 출력하기
package kr.co.infopub.chapter.s054;

// 향상된 for와 배열
public class ForeachLotto {

	public static void main(String[] args) {
		int [] mm = {1, 6, 16, 22, 23, 33};
		
		for(int m : mm) {
			System.out.printf(m + "\t");
		}
		System.out.println();
		
		int [] mm = {1, 6, 16, 22, 23, 33};
		
		for(int i = 0; i < mm.length; i++) {
			int m = mm[i];
			System.out.printf(m + "\t");
		}
		System.out.println();
	}
}
